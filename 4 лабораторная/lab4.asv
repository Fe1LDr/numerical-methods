clear
clc
clf

y=@(x) cos(3*x);
h1=10^(-3);
h2=10^(-6);
x0=0.8;
dy1_h1=proiz(y,x0,h1)
dy1_h2=proiz(y,x0,h2)

i=[1:1:10];
h=2.^(-i);
x0=0.8;
etalon=proiz(y,x0,10^(-10))
for j=1:1:length(h)
[dy1(j),dy2(j)]=proiz(y,x0,h(j));
delta1(j)=abs(dy1(j)-etalon);
delta2(j)=abs(dy2(j)-etalon);
end
dy1
dy2

hold on
grid on
axis square
plot(h,delta1,'o')
plot(h,delta2,'o')
fplot(@(x) 1/25*x^2,[0 1])